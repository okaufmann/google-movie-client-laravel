{
    "name": "okaufmann/google-movies-client-laravel",
    "description": "Package to integrate google movies client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Kaufmann",
            "email": "support@mighty-code.com",
            "homepage": "https://github.com/okaufmann"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.0",
        "illuminate/support": "5.0.*|5.1.*|5.2.*",
        "okaufmann/google-movies-client": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "symfony/event-dispatcher": "~2.0",
        "illuminate/events": "~5.0",
        "doctrine/cache": ">=1.3.0",
        "monolog/monolog": "1.*"
    },
    "suggest": {
        "doctrine/cache": "Required if you want to make use of caching features.",
        "monolog/monolog": "Required if you want to make use of logging features."
    },
    "autoload": {
        "psr-4": {
            "GoogleMoviesClient\\Laravel\\": "src/"
        }
    }
}
